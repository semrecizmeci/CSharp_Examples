using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        List<int> ints = new List<int>() { 1, 2, 3, 4, 5, 6, 7, 8, 9 };

        bool containsMatch = new[] { 3, 7, 9 }.Any(x => ints.Contains(x));

        Console.WriteLine(containsMatch); // true
    }
}
//Contains metodu, belirli bir değerin koleksiyon içinde bulunup bulunmadığını kontrol eder.
